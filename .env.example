# 这是一个环境变量示例文件，请复制并重命名为.env或.env.local使用
# 注意：敏感密钥不应提交到版本控制系统中

# Supabase配置
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret

# 应用配置
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_VERCEL_URL=http://localhost:3000
NEXT_PUBLIC_SUPABASE_DEBUG=true

# 代理设置（如需）
NO_PROXY=localhost,127.0.0.1
HTTP_PROXY=
HTTPS_PROXY=
http_proxy=
https_proxy=

# 日志级别设置
LOG_LEVEL=INFO

# OpenAI配置
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4-turbo

# 图资API配置
OPENAI_BASE_URL=https://api.tu-zi.com/v1
OPENAI_IMAGE_MODEL=gpt-4o-image-vip

# 任务处理密钥配置 - 非常重要
# 用于任务服务之间相互认证
TASK_PROCESS_SECRET_KEY=your-task-process-secret-key
INTERNAL_API_KEY=your-internal-api-key

# 支付系统配置
PAYMENT_WEBHOOK_SECRET=your-payment-webhook-secret

# 定时任务配置
CRON_API_KEY=your-cron-api-key

# 支付相关
ZPAY_PID=your-payment-id
NEXT_PUBLIC_ZPAY_PID=your-payment-id

# 离线模式控制
FORCE_ONLINE_MODE=false
DISABLE_OFFLINE_FALLBACK=false

# HTTP请求设置
HTTP_TIMEOUT=60000
HTTP_CONNECTION_TIMEOUT=30000

# 环境标识
NODE_ENV=development
VERCEL=1

# 认证相关
NEXTAUTH_SECRET=your-nextauth-secret
JWT_SECRET=your-jwt-secret

# 图片处理设置
IMAGE_COMPRESS_QUALITY=80
IMAGE_MAX_SIZE=10485760

# 文件大小限制
MAX_REQUEST_SIZE=10485760
MAX_UPLOAD_SIZE=5242880 