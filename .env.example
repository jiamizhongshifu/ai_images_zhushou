# 数据库配置
NEXT_PUBLIC_SUPABASE_URL=https://example.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret

# 站点配置
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_VERCEL_URL=http://localhost:3000
NEXT_PUBLIC_SUPABASE_DEBUG=true

# 代理设置（如需）
NO_PROXY=localhost,127.0.0.1
HTTP_PROXY=
HTTPS_PROXY=
http_proxy=
https_proxy=

# 日志级别设置
LOG_LEVEL=INFO

# OpenAI API配置
OPENAI_API_KEY=your-api-key
OPENAI_BASE_URL=https://api.tu-zi.com/v1

# 模型配置（分离聊天模型和图像生成模型）
OPENAI_CHAT_MODEL=gpt-4o-all       # 用于聊天功能的模型
OPENAI_IMAGE_MODEL=dall-e-3        # 用于图像生成的模型

# 支付相关
ZPAY_PID=your-payment-id
NEXT_PUBLIC_ZPAY_PID=your-payment-id

# 任务处理
TASK_PROCESS_SECRET_KEY=dev-secret-key

# API安全
INTERNAL_API_KEY=your-internal-api-key
PAYMENT_WEBHOOK_IP_WHITELIST=127.0.0.1,::1

# 离线模式控制
FORCE_ONLINE_MODE=false
DISABLE_OFFLINE_FALLBACK=false

# HTTP请求设置
HTTP_TIMEOUT=60000
HTTP_CONNECTION_TIMEOUT=30000

# 环境标识
NODE_ENV=development
VERCEL=1

# 认证相关
NEXTAUTH_SECRET=your-nextauth-secret
JWT_SECRET=your-jwt-secret

# 图片处理设置
IMAGE_COMPRESS_QUALITY=80
IMAGE_MAX_SIZE=10485760 